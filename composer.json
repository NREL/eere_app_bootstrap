{
  "name": "nrel/eere_app_bootstrap",
  "description": "EERE branded Drupal 8 bootstrap parent theme.",
  "type": "drupal-theme",
  "keywords": ["drupal", "bootstrap", "eere"],
  "license": "GPL-2.0",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "homepage": "https://github.com/NREL/eere_app_bootstrap",
  "authors": [
    {
      "name": "Shaun Laws (slaws)",
      "email": "shaun.laws@nrel.gov",
      "role": "Co-maintainer"
    },
    {
      "name": "Christian Modern (cmodern)",
      "email": "christian.modern@nrel.gov",
      "role": "Co-maintainer"
    }
  ],
  "support": {
    "issues": "https://github.com/NREL/eere_app_bootstrap/issues"
  },
  "require-dev": {
    "nuvoleweb/drupal-component-scaffold": "*",
    "drupal/console": "~1",
    "drush/drush": "*",
    "drupal/core": "~8"
  },
  "require": {
    "nrel/eere_app_module": "1.0.1",
    "drupal/bootstrap": "~3.13",
    "drupal/bootstrap_layouts": "^5.1",
    "twbs/bootstrap": "^3.3.7"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    "packages.drupal.org": false
  },
  "extra": {
    "installer-paths": {
      "build/core": ["type:drupal-core"],
      "build/modules/contrib/{$name}": ["type:drupal-module"],
      "build/profiles/contrib/{$name}": ["type:drupal-profile"],
      "build/themes/contrib/{$name}": ["type:drupal-theme"]
    }
  }
}
