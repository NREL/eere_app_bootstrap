{#
/**
* @file
* Default theme implementation for a 'form' element.
*
* Available variables
* - attributes: A list of HTML attributes for the wrapper element.
* - children: The child elements of the form.
*
* @see template_preprocess_form()
*
* @ingroup themeable
*/
#}
<form{{ attributes }} class="navbar-form navbar-left hidden-sm hidden-xs" role="search">
  <div class="form-group">
    {{ children }}
  </div>
  <a class="btn btn-navbar"><i class="fa fa-search" aria-hidden="true"></i></a>
</form>
